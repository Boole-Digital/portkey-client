{
    "name": "portara-vault-module",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "serve:test": "serve -l 3000 .",         
        "serve:vault": "serve public -l 3001",
        "serve:all": "concurrently \"npm run serve:test\" \"npm run serve:vault\"",
        "build": "tsc",
        "move": "cp dist/PortaraVault.js test/PortaraVault.js",
        "start": "npm run build && npm run move && concurrently \"npm run serve:test\" \"npm run serve:vault\""
    },
    "devDependencies": {
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.3",
        "concurrently": "^8.0.1",
        "serve": "^14.0.1",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "react": "^19.1.0",
        "react-dom": "^19.1.0"
    }
}
