{
    "name": "portkey-client",
    "version": "1.0.2",
    "description": "React provider to embed a persistent iframe as a background layer.",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "node build.mjs",
        "type-check": "tsc --noEmit",
        "start-vault": "serve public -l 3002",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Boole-Digital/portkey"
    },
    "keywords": [
        "PRF",
        "passkey",
        "web3",
        "wallet",
        "EVM",
        "Solana"
    ],
    "author": "Lachlan G",
    "license": "MIT",
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@solana/web3.js": "^1.98.2",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "bs58": "^6.0.0",
        "esbuild": "^0.25.5",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-router-dom": "^7.6.2",
        "serve": "^14.2.4",
        "typescript": "^5.0.0"
    }
}
