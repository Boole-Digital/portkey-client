{
    "name": "portkey-client",
    "version": "1.0.2",
    "description": "React provider to embed a persistent iframe as a background layer.",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
    ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
    }
    },
