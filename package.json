        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "serve": "^14.2.4",
        "typescript": "^5.0.0",
        "@solana/web3.js": "^1.98.2",
        "bs58": "^6.0.0",
        "esbuild": "^0.25.5",
        "react-router-dom": "^7.6.2"
    }
